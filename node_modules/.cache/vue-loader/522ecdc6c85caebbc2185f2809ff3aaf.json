eft\">\n      <!-- 左侧目录 -->\n      <el-menu\n        :default-active=\"activeIndex\"\n        class=\"el-menu-demo\"\n        mode=\"horizontal\"\n        @select=\"handleSelect\"\n      >\n        <el-menu-item index=\"1\">处理中心</el-menu-item>\n        <el-menu-item index=\"2\">我的工作台 </el-menu-item>\n        <el-menu-item index=\"3\">\n          <a href=\"https://www.ele.me\" target=\"_blank\"\n            >订单管理</a\n          ></el-menu-item\n        >\n      </el-menu>\n    </div>\n    <div\n      style=\"\n        overflow: hidden;\n        zoom: 1;\n        width: 100%;\n        border-left: 1px solid #dcdfe6;\n      \"\n    >\n      <!-- 右侧内容 -->\n      <div style=\"height: 10px\">&nbsp;</div>\n      <router-view> </router-view>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { version } from \"@/api/user\";\nexport default {\n  name: \"HelloWorld\",\n  data() {\n    return { versionText: \"\", activeIndex: \"1\" };\n  },\n  mounted() {\n    browser.runtime.sendMessage({});\n\n    version().then((res) => {\n      console.warn(33, res);\n      window.alert(this.$route.path);\n      this.versionText = res;\n    });\n  },\n  computed: {\n    defaultText() {\n      return browser.i18n.getMessage(\"extName\");\n    }\n  },\n  methods: {\n    handleSelect(key, keyPath) {\n      window.alert(key + typeof key + JSON.stringify(this.$route));\n      if (key === \"1\") {\n        alert(2);\n        this.$router.replace({ path: \"/actionCenter\" });\n        alert(3 + this.$route.path, JSON.stringify(this.$route));\n      } else if (key === \"2\") {\n        alert(4);\n        this.$router.replace({ path: \"/myWork\" });\n        alert(\n          5 + this.$route.path + \" %%%%%%%%% \" + JSON.stringify(this.$router)\n        );\n      }\n      console.log(key, keyPath);\n    }\n  }\n};\n</script>\n\n<style scoped>\np {\n  font-size: 20px;\n}\n</style>\n"]}]}